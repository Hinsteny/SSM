    <typeAliases>
    <typeAlias alias="CamelbakKeyResultMap" type="com.hisoka.result.CamelbakKeyResultMap"/>
    </typeAliases>
    <objectWrapperFactory type="com.hisoka.result.CamelbakKeyMapWrapperFactory"></objectWrapperFactory>
